{
  "tanstack-router": {
    "intent_patterns": [
      "create.*route",
      "add.*route",
      "implement.*navigation",
      "create.*loader",
      "add.*route.*parameter"
    ],
    "keyword_patterns": [
      "tanstack router",
      "createFileRoute",
      "useNavigate",
      "useParams",
      "route loader",
      "@tanstack/react-router"
    ],
    "content_patterns": [
      "createFileRoute\\(",
      "import.*@tanstack/react-router"
    ],
    "file_path_patterns": [
      "**/routes/**/*.tsx",
      "**/routes/**/*.ts"
    ]
  }
}
