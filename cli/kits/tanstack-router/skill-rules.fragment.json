{
  "skills": {
    "tanstack-router": {
      "type": "domain",
      "enforcement": "suggest",
      "priority": "medium",
      "promptTriggers": {
        "keywords": [
          "TanStack Router",
          "routing",
          "route",
          "navigation",
          "Link",
          "navigate",
          "router"
        ],
        "intentPatterns": [
          "(create|add).*?(route|page|navigation)",
          "(navigate|link).*?to"
        ]
      },
      "fileTriggers": {
        "pathPatterns": [
          "src/routes/**/*.tsx",
          "routes/**/*.tsx"
        ],
        "contentPatterns": [
          "from '@tanstack/react-router'",
          "createFileRoute",
          "useNavigate",
          "<Link"
        ]
      }
    }
  }
}
