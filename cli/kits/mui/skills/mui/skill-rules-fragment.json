{
  "mui": {
    "intent_patterns": [
      "style.*with.*mui",
      "use.*mui.*component",
      "create.*mui.*theme",
      "customize.*mui"
    ],
    "keyword_patterns": [
      "mui",
      "material-ui",
      "material ui",
      "@mui/material",
      "sx prop",
      "theme",
      "Box",
      "Typography",
      "Button"
    ],
    "content_patterns": [
      "import.*@mui/material",
      "sx=\\{",
      "<Box",
      "<Typography"
    ],
    "file_path_patterns": [
      "**/*.styles.ts",
      "**/*.styles.tsx"
    ]
  }
}
