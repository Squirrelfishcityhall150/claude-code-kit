{
  "express": {
    "intent_patterns": [
      "create.*express.*route",
      "add.*express.*middleware",
      "create.*api.*endpoint",
      "implement.*controller"
    ],
    "keyword_patterns": [
      "express",
      "express.Router",
      "app.get",
      "app.post",
      "middleware",
      "req.body",
      "res.json"
    ],
    "content_patterns": [
      "import.*express",
      "Router\\(\\)",
      "app\\.(get|post|put|delete)"
    ],
    "file_path_patterns": [
      "**/routes/**/*.ts",
      "**/controllers/**/*.ts",
      "**/middleware/**/*.ts",
      "**/app.ts",
      "**/server.ts"
    ]
  }
}
